Xft.dpi: 96

XTerm*decTerminalID: vt340
XTerm*numColorRegisters: 256
XTerm.termName: xterm-256color
XTerm.vt100.metaSendsEscape: true
XTerm.vt100.backarrowKey: false
XTerm.vt100.locale: true
XTerm.ttyModes: erase ^?
XTerm.vt100.faceName: xos4 Terminus:size=6:antialias=false

! blue
*.color4:       #6495ed
*.color12:      #87ceeb

URxvt.font: xft:xos4 Terminus:antialias=true,xft:FontAwesome:size=8:antialias=true
URxvt.letterSpace: -1
URxvt.scrollBar: false

rofi.font: mono 12
rofi.color-enabled: true
rofi.color-window: #171717, #ffdf00, #ffdf00
rofi.color-normal: #000000, #ffdf00, #171717, #ffdf00, #000000
rofi.color-active: #000000, #268bd2, #171717, #268bd2, #fdf6e3
rofi.color-urgent: #000000, #dc322f, #171717, #dc322f, #fdf6e3
rofi.terminal: st
